package javaday9ppnewc;

public class Student {

	int rno;
	String sname;
	double per;
	boolean isPnrActive;

	public Student() {
		System.out.println("Student values are passed inside constructor so default constructor will not called.");
		this.rno = 1;
		this.sname = "Alice";
		this.per = 40.00;
		this.isPnrActive = true;
	}
}
