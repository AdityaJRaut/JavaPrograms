import java.util.Iterator;

public class trianglePattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<=9;i++)
		{
			for(int j=1;j<=9;j++)
			{
				if(i<=4+j && i>=6-j)
				{
					System.out.println("* ");
				}
				else
				{
					System.out.println("  ");
				}
			}
			System.out.println("\n");
		}
	}

}
