package dsa;

public class Fibonacci {

	public static void main(String[] args) {
		
		int n1=0,n2=1;
		System.out.print(n1+","+n2+",");
		Fibonacci(n1, n2);
		
	}

	private static void Fibonacci(int n1, int n2) 
	{
		int n3;
		for(int i=2;i<12;i++)
		{
			n3=n1+n2;
			System.out.print(n3+",");
			n1=n2;
			n2=n3;
			Fibonacci(n1,n2);
		}
		
	}
	

}
