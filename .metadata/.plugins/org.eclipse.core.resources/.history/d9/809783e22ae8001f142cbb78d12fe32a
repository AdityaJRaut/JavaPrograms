package dsa;

import java.util.Arrays;

public class BubbleSort {

	public static void main(String[] args) {
		int[] arr= {7,12,9,11,3};
		for(int i=0;i<5;i++)
		{
			for(int j=0;j<5;j++)
			{
				if(arr[j]>arr[j+1])
				{
					int temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=temp;
				}
			}
		}
		System.out.print(Arrays.toString(arr));
		

	}

}
